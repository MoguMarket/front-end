{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.*",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://www.jungjiyu.com/api/:path*"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
